        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="AA_DB_NAME"
            android:value="mimo.db"/>
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1"/>
